<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="colorOnPrimary" format="color" />
    <attr name="colorOnPrimarySecondary" format="color" />
    <attr name="colorAccentDark" format="color" />
    <attr name="colorOnAccent" format="color" />
    <attr name="colorBackground" format="color" />
    <attr name="colorOnBackground" format="color" />
    <attr name="colorSurface" format="color" />
    <attr name="colorOnSurface" format="color" />

    <declare-styleable name="RateSpinnerView">
        <attr name="anime" format="boolean" />
        <attr name="showEdit" format="boolean" />
        <attr name="showIcon" format="boolean" />
        <attr name="itemSize" format="enum">
            <enum name="small" value="0"/>
            <enum name="normal" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NetworkErrorView">
        <attr name="text" format="string" />
    </declare-styleable>
</resources>
